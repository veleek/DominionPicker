<?xml version="1.0" encoding="utf-8" ?> 
<MainViewModel xmlns="clr-namespace:Ben.Dominion" xmlns:p="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
               xmlns:system="clr-namespace:System;assembly=mscorlib">
    <MainViewModel.Result>
        <PickerResult xmlns="clr-namespace:Ben.Dominion">
            <PickerResult.Cards>
                <Card Name="Archive" DisplayName="L_Archive" BackgroundColor="#A09BA5" CoinCost="0" Cost="5" Set="Base" ID="100" Type="Action, Attack" />
                <Card Name="Sample B Card" DisplayName="L_Sample B Card" BackgroundColor="#A09BA5" CoinCost="1" Cost="5" Set="Intrigue" ID="100" Type="Action, Attack" />
