                <Card Name="Scrying Pool" DisplayName="L_Scrying Pool"BackgroundColor="#A09BA5" CoinCost="6" Cost="5" Set="Hinterlands" ID="100" Type="Action, Attack" />
                <Card Name="Silk Road" DisplayName="L_Silk Road"BackgroundColor="#A09BA5" CoinCost="7" Cost="5" Set="DarkAges" ID="100" Type="Action, Attack" />
                <Card Name="Bridge" DisplayName="L_Bridge"BackgroundColor="#A09BA5" CoinCost="8" Cost="5" Set="DarkAges" ID="100" Type="Action, Attack" />
                <Card Name="Moat" DisplayName="L_Moat"BackgroundColor="#BC8010" CoinCost="9" Cost="5" Set="DarkAges" ID="100" Type="Action, Attack" HasPotion="True"/> 
                -->
            </PickerResult.Cards>
            <PickerResult.AdditionalCards>
                <Card Name="Trader" DisplayName="L_Trader" BackgroundColor="#A09BA5" CoinCost="7" Cost="5" Set="DarkAges" ID="100" Type="Action, Attack" />
            </PickerResult.AdditionalCards>
            <PickerResult.AdditionalStuff>
