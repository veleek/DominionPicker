                <Card Name="Elephant Bones" DisplayName="L_Elephant Bones" CoinCost="4" Cost="5" Set="Prosperity" ID="100" Type="Action, Attack">
                    <Card.BackgroundColor>
                        <p:LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5, 1">
                            <p:LinearGradientBrush.GradientStops>
                                <p:GradientStop Color="#EBB40F" Offset="0"/>
                                <p:GradientStop Color="#EBB40F" Offset="0.2"/>
                                <p:GradientStop Color="#617939" Offset="0.8"/>
                                <p:GradientStop Color="#617939" Offset="1"/>
                            </p:LinearGradientBrush.GradientStops>
                        </p:LinearGradientBrush>
                    </Card.BackgroundColor>
                </Card>
                <Card Name="Philosopher's Stone" DisplayName="L_Philosopher's Stone" BackgroundColor="#A09BA5" CoinCost="5" Cost="5" Set="Cornucopia" ID="100" Type="Action, Attack" HasPotion="True"/>
